v005A2B20_0 .net "clk", 0 0, C4<z>; 0 drivers
v005A3B70_0 .net "in", 31 0, v00703448_0; 1 drivers
v005A3BC8_0 .var "out", 31 0;
v005A3C20_0 .net "reset", 0 0, v007034F8_0; 1 drivers
E_005A1ED0 .event posedge, v005A2B20_0;
    .scope S_00707890;
T_0 ;
    %wait E_005A1ED0;
    %load/v 8, v005A3C20_0, 1;
    %mov 9, 0, 1;
    %cmpi/u 8, 0, 2;
    %jmp/0xz  T_0.0, 4;
    %ix/load 0, 32, 0;
    %assign/v0 v005A3BC8_0, 0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v005A3B70_0, 32;
    %ix/load 0, 32, 0;
    %assign/v0 v005A3BC8_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_00707010;
T_1 ;
    %set/v v007034F8_0, 0, 1;
    %set/v v00703448_0, 0, 32;
    %delay 100, 0;
    %movi 8, 2, 32;
    %set/v v00703448_0, 8, 32;
    %delay 100, 0;
    %movi 8, 3, 32;
    %set/v v00703448_0, 8, 32;
    %delay 100, 0;
    %set/v v007034F8_0, 1, 1;
    %movi 8, 2, 32;
    %set/v v00703448_0, 8, 32;
    %delay 100, 0;
    %movi 8, 3, 32;
    %set/v v00703448_0, 8, 32;
    %end;
    .thread T_1;
    .scope S_00707010;
T_2 ;
    %delay 700, 0;
    %vpi_call 2 18 "$finish";
    %end;
    .thread T_2;
    .scope S_00707010;
T_3 ;
    %vpi_call 2 21 "$monitor", "Reset = %d", v007034F8_0, " In = %d", v00703448_0, " Out = %d", v007034A0_0;
