S_00378098 .scope module, "pc_tb" "pc_tb" 2 2;
 .timescale -9 -10;
v00373400_0 .net "count", 31 0, v003787F8_0; 1 drivers
v00373458_0 .var "in", 31 0;
v003734B0_0 .var "reset", 0 0;
S_00377A38 .scope module, "mydut" "pc" 2 7, 3 1, S_00378098;
 .timescale 0 0;
v005A3A08_0 .net "clk", 0 0, C4<z>; 0 drivers
v003787F8_0 .var "count", 31 0;
v00378850_0 .net "in", 31 0, C4<zzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzz>; 0 drivers
v003788A8_0 .net "reset", 0 0, v003734B0_0; 1 drivers
E_003772B8/0 .event negedge, v003788A8_0;
E_003772B8/1 .event posedge, v005A3A08_0;
E_003772B8 .event/or E_003772B8/0, E_003772B8/1;
    .scope S_00377A38;
T_0 ;
    %wait E_003772B8;
    %load/v 8, v003788A8_0, 1;
    %inv 8, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 32, 0;
    %assign/v0 v003787F8_0, 0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v00378850_0, 32;
    %ix/load 0, 32, 0;
    %assign/v0 v003787F8_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_00378098;
T_1 ;
    %set/v v003734B0_0, 1, 1;
    %set/v v00373458_0, 0, 32;
    %delay 100, 0;
    %set/v v003734B0_0, 1, 1;
    %delay 100, 0;
    %set/v v00373458_0, 0, 32;
    %vpi_call 2 12 "$display", "Reset = %d", v003734B0_0, " Counter = %d", v00373400_0;
    %delay 100, 0;
    %set/v v003734B0_0, 1, 1;
    %delay 100, 0;
    %movi 8, 4, 32;
    %set/v v00373458_0, 8, 32;
    %vpi_call 2 15 "$display", "Reset = %d", v003734B0_0, " Counter = %d", v00373400_0;
    %delay 100, 0;
    %set/v v003734B0_0, 1, 1;
    %delay 100, 0;
    %movi 8, 8, 32;
    %set/v v00373458_0, 8, 32;
    %vpi_call 2 18 "$display", "Reset = %d", v003734B0_0, " Counter = %d", v00373400_0;
    %delay 100, 0;
    %set/v v003734B0_0, 1, 1;
    %delay 100, 0;
    %set/v v00373458_0, 0, 32;
    %vpi_call 2 21 "$display", "Reset = %d", v003734B0_0, " Counter = %d", v00373400_0;
    %delay 100, 0;
    %set/v v003734B0_0, 1, 1;
    %delay 100, 0;
    %movi 8, 4, 32;
    %set/v v00373458_0, 8, 32;
    %vpi_call 2 24 "$display", "Reset = %d", v003734B0_0, " Counter = %d", v00373400_0;
