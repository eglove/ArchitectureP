#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 10;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_006F3878 .scope module, "pc_tb" "pc_tb" 2 2;
 .timescale -9 -10;
v00863388_0 .net "count", 31 0, v008632D8_0; 1 drivers
v00862AC0_0 .var "reset", 0 0;
S_006F3900 .scope module, "mydut" "pc" 2 6, 3 1, S_006F3878;
 .timescale 0 0;
v008672D8_0 .net "clk", 0 0, C4<z>; 0 drivers
v008632D8_0 .var "count", 31 0;
v00863330_0 .net "reset", 0 0, v00862AC0_0; 1 drivers
E_006F1C90/0 .event negedge, v00863330_0;
E_006F1C90/1 .event posedge, v008672D8_0;
E_006F1C90 .event/or E_006F1C90/0, E_006F1C90/1;
    .scope S_006F3900;
T_0 ;
    %wait E_006F1C90;
    %load/v 8, v00863330_0, 1;
    %jmp/0xz  T_0.0, 8;
    %set/v v008632D8_0, 0, 32;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v008632D8_0, 32;
    %mov 40, 0, 1;
    %addi 8, 4, 33;
    %set/v v008632D8_0, 8, 32;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_006F3878;
T_1 ;
    %set/v v00862AC0_0, 0, 1;
    %delay 100, 0;
    %set/v v00862AC0_0, 1, 1;
    %vpi_call 2 10 "$display", "Reset = %d", v00862AC0_0, " Counter = %d", v00863388_0;
    %delay 100, 0;
    %set/v v00862AC0_0, 1, 1;
    %vpi_call 2 12 "$display", "Reset = %d", v00862AC0_0, " Counter = %d", v00863388_0;
    %delay 100, 0;
    %set/v v00862AC0_0, 1, 1;
    %vpi_call 2 14 "$display", "Reset = %d", v00862AC0_0, " Counter = %d", v00863388_0;
    %delay 100, 0;
    %set/v v00862AC0_0, 0, 1;
    %vpi_call 2 16 "$display", "Reset = %d", v00862AC0_0, " Counter = %d", v00863388_0;
    %delay 100, 0;
    %set/v v00862AC0_0, 0, 1;
    %vpi_call 2 18 "$display", "Reset = %d", v00862AC0_0, " Counter = %d", v00863388_0;
    %end;
    .thread T_1;
    .scope S_006F3878;
T_2 ;
    %delay 1000, 0;
    %vpi_call 2 22 "$finish";
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
