v00882AD8 .array "words", 7 0, 31 0;
E_00732378 .event posedge, v00883CC0_0;
    .scope S_00887DE0;
T_0 ;
    %movi 8, 10, 32;
   %ix/load 1, 0, 0;
   %ix/load 3, 0, 0;
   %set/av v00882AD8, 8, 32;
    %movi 8, 20, 32;
   %ix/load 1, 0, 0;
