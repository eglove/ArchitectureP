   %set/av v00882AD8, 8, 32;
    %movi 8, 50, 32;
   %ix/load 1, 0, 0;
   %ix/load 3, 4, 0;
   %set/av v00882AD8, 8, 32;
    %movi 8, 60, 32;
   %ix/load 1, 0, 0;
   %ix/load 3, 5, 0;
   %set/av v00882AD8, 8, 32;
    %movi 8, 70, 32;
   %ix/load 1, 0, 0;
   %ix/load 3, 6, 0;
   %set/av v00882AD8, 8, 32;
    %movi 8, 80, 32;
   %ix/load 1, 0, 0;
   %ix/load 3, 7, 0;
   %set/av v00882AD8, 8, 32;
    %end;
    .thread T_0;
    .scope S_00887DE0;
T_1 ;
    %wait E_00732378;
    %ix/getv 3, v00883C10_0;
    %load/av 8, v00882AD8, 32;
    %ix/load 0, 32, 0;
    %assign/v0 v00883C68_0, 0, 8;
    %jmp T_1;
    .thread T_1;
    .scope S_00887780;
T_2 ;
    %set/v v00888C80_0, 0, 1;
    %set/v v00882B30_0, 0, 3;
    %set/v v00888CD8_0, 0, 4;
T_2.0 ;
    %load/v 8, v00888CD8_0, 4;
    %mov 12, 0, 2;
   %cmpi/u 8, 8, 6;
    %jmp/0xz T_2.1, 5;
    %load/v 8, v00888CD8_0, 4;
    %set/v v00882B30_0, 8, 3;
    %delay 10000, 0;
    %vpi_call 2 23 "$display", $time, " Address = %d", v00882B30_0, " Instruction = %d", v00882B88_0;
    %load/v 8, v00888CD8_0, 4;
    %mov 12, 0, 28;
    %addi 8, 1, 32;
    %set/v v00888CD8_0, 8, 4;
    %jmp T_2.0;
T_2.1 ;
    %end;
    .thread T_2;
    .scope S_00887780;
T_3 ;
    %delay 1000, 0;
    %load/v 8, v00888C80_0, 1;
    %inv 8, 1;
    %set/v v00888C80_0, 8, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_00887780;
T_4 ;
